params {
  config_profile_description = 'New York University HPC cluster profile provided by nf-core/configs.'
  config_profile_contact = 'HPC@nyu.edu'
  config_profile_url = 'https://hpc.nyu.edu'
}

profiles {
  slurm {
    params {
      max_memory = 3000.GB
      max_cpus = 96
      max_time = 7.d
    }
    process {
      executor = 'slurm'
      maxRetries = 2
    }
    executor {
      queueSize = 250
      submitRateLimit = '10 sec'
    }
  }
}
